String s="saibharath";
    int freq[]=new int[26];
   
    for(int i=0;i<s.length();i++){
        freq[s.charAt(i)-'a']++;
    }
    
    for(int i=0;i<s.length();i++){
    if(freq[s.charAt(i)-'a']!=0){
        System.out.print(s.charAt(i)+" ");
        System.out.println(freq[s.charAt(i)-'a']);
        freq[s.charAt(i)-'a']=0;
        
    }
    }
	
